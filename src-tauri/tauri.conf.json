{
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "mussia18",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "yurikrupnik.com",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "mussia18",
        "width": 800,
        "height": 600
      }
    ],
    "cli": {
      "description": "command description that's shown on help",
      "longDescription": "command long description that's shown on help",
      "beforeHelp": "content to show before the help text",
      "afterHelp": "content to show after the help text",
      "args": [

      ],
      "subcommands": {
        "branch": {
          "args": []
        },
        "push": {
          "args": []
        },
        "cluster": {},
        "subcommand-name": {}
      }
    }
  }
}
